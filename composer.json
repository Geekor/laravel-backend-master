{
    "name": "geekor/laravel-backend-master",
    "description": "Geekor's laravel backend for v9.x",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Geekor",
            "email": "geekor@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Geekor\\BackendMaster\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geekor\\BackendMaster\\Database\\Factories\\": "database/factories/",
            "Geekor\\BackendMaster\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "geekor/laravel-gk-core": "dev-main",
        "illuminate/container": "^6.9|^7.0|^8.0|^9.0",
        "illuminate/database": "^6.9|^7.0|^8.0|^9.0",
        "illuminate/http": "^6.9|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.9|^7.0|^8.0|^9.0",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Geekor\\BackendMaster\\ServiceProvider"
            ]
        }
    }
}
